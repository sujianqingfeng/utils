{
  "name": "@sujian/utils-mono",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "description": "my frontend utils",
  "author": "hens",
  "license": "MIT",
  "homepage": "https://github.com/sujianqingfeng/utils",
  "keywords": [
    "frontend utils"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*",
        "./lib/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "npm publish --no-git-checks --access public"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@sujian/eslint-config-typescript": "^0.0.17",
    "eslint": "^8.23.0",
    "happy-dom": "^6.0.4",
    "tsup": "^6.4.0",
    "typescript": "^4.8.2",
    "vitest": "^0.23.4"
  }
}
