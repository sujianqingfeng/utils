{
  "name": "@sujian/utils",
  "version": "1.0.7",
  "description": "my frontend utils",
  "author": "hens",
  "license": "MIT",
  "homepage": "https://github.com/sujianqingfeng/utils",
  "keywords": [
    "frontend utils"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "npm publish --no-git-checks --access public"
  },
  "dependencies": {
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.5"
  },
  "devDependencies": {
    "@sujian/eslint-config-typescript": "^0.0.17",
    "@swc/core": "^1.2.246",
    "@types/big.js": "^6.1.5",
    "eslint": "^8.23.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "vitest": "^0.23.4"
  }
}
