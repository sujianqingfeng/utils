{
  "name": "@sujian/utils",
  "type": "module",
  "version": "1.0.13",
  "description": "my frontend utils",
  "author": "hens",
  "license": "MIT",
  "homepage": "https://github.com/sujianqingfeng/utils",
  "keywords": [
    "frontend utils"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*",
        "./lib/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "npm publish --no-git-checks --access public"
  },
  "dependencies": {
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.5"
  },
  "devDependencies": {
    "@sujian/eslint-config-typescript": "^0.0.17",
    "eslint": "^8.23.0",
    "happy-dom": "^6.0.4",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "vitest": "^0.23.4"
  }
}
