{
  "name": "@sujian/utils-mono",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "description": "my frontend utils",
  "author": "hens",
  "license": "MIT",
  "homepage": "https://github.com/sujianqingfeng/utils",
  "keywords": [
    "frontend utils"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "change-version": "bumpp package.json packages/*/package.json --no-commit --no-push --no-tag",
    "release": "pnpm -r publish --no-git-checks --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "devDependencies": {
    "@sujian/eslint-config-typescript": "^0.0.17",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.23.0",
    "happy-dom": "^6.0.4",
    "tsup": "^6.4.0",
    "typescript": "^4.8.2",
    "vitest": "^0.23.4"
  }
}
